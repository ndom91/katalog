{
  "name": "katalog",
  "version": "0.1.0",
  "description": "Inventory Management Application",
  "scripts": {
    "dev": "next",
    "build": "nexus build && next build",
    "start": "next start -H 0.0.0.0",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/pages/api",
    "postinstall": "npm -s run generate",
    "prisma:migrate:save": "prisma migrate save --name lagerify --experimental",
    "prisma:migrate:up": "prisma migrate up --experimental"
  },
  "keywords": [
    "warehouse",
    "inventory",
    "management"
  ],
  "author": "Nico Domino <yo@ndo.dev>",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@apollo/react-hooks": "3.1.5",
    "@apollo/react-ssr": "3.1.5",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.4.2",
    "antd-img-crop": "^3.9.0",
    "apollo-boost": "0.4.9",
    "apollo-link-http": "1.5.17",
    "apollo-link-schema": "1.2.5",
    "apollo-server-micro": "^2.16.0",
    "dayjs": "^1.8.29",
    "express": "4.17.1",
    "glob": "^7.1.6",
    "graphql-iso-date": "3.6.1",
    "graphql-tag": "2.10.4",
    "isomorphic-unfetch": "3.0.0",
    "next": "9.4.4",
    "next-auth": "^2.2.0",
    "next-react-svg": "^1.1.2",
    "nexus": "^0.25.0",
    "nexus-plugin-prisma": "^0.16.1",
    "nexus-prisma": "^0.12.0",
    "qrcode.react": "^1.0.0",
    "react": "16.13.1",
    "react-apollo": "3.1.5",
    "react-dom": "16.13.1",
    "react-feather": "^2.0.8",
    "react-markdown": "4.3.1",
    "sqlite3": "^5.0.0",
    "styled-components": "^5.1.1",
    "typescript-plugin-css-modules": "^2.4.0"
  },
  "devDependencies": {
    "@types/graphql": "14.5.0",
    "@types/node": "13.13.12",
    "@types/react": "16.9.41",
    "@types/styled-components": "^5.1.1",
    "babel-plugin-styled-components": "^1.10.7",
    "copy-webpack-plugin": "^6.0.3",
    "null-loader": "^4.0.0",
    "prettier": "^2.0.5",
    "riek": "^1.1.0",
    "typescript": "3.9.6"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": false,
    "jsxSingleQuote": true,
    "arrowParens": "avoid"
  }
}
