    "null-loader": "^4.0.1",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "4.0.5",
    "typescript-plugin-css-modules": "^2.7.0"
  },
