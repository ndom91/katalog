{
  "name": "katalog",
  "version": "0.5.0",
  "description": "Inventory Management Application",
  "scripts": {
    "dev": "next",
    "build": "nexus build && next build",
    "start": "next start -H 0.0.0.0 -p 80",
    "nexus:dev": "npx nexus dev",
    "prisma:studio": "npx prisma studio --experimental",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/pages/api",
    "postinstall": "npm -s run generate",
    "prisma:migrate:save": "prisma migrate save --name lagerify --experimental",
    "prisma:migrate:up": "prisma migrate up --experimental",
    "prisma:docs:serve": "npm run generate && npx prisma-docs-generator serve",
    "db:update": "npm run generate && npm run prisma:migrate:save && npm run prisma:migrate:up",
    "prettier": "prettier --config .prettierrc --check \"**/*.{js,jsx,ts,tsx,css,html}\"",
    "prettier:fix": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx,css,html}\""
  },
  "homepage": "https://github.com/ndom91/lagerify",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndom91/lagerify"
  },
  "bugs": {
    "url": "https://github.com/ndom91/lagerify/issues"
  },
  "keywords": [
    "warehouse",
    "inventory",
    "management"
  ],
  "author": "Nico Domino <yo@ndo.dev>",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@apollo/client": "^3.1.3",
    "@loadable/component": "^5.13.1",
    "@loadable/server": "^5.13.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.5.2",
    "antd-img-crop": "^3.9.0",
    "aws-sdk": "^2.742.0",
    "dayjs": "^1.8.32",
    "glob": "^7.1.6",
    "graphql-iso-date": "3.6.1",
    "isomorphic-unfetch": "3.0.0",
    "lodash.difference": "^4.5.0",
    "next": "^10.0.0",
    "next-auth": "^3.1.0",
    "next-react-svg": "^1.1.2",
    "nexus": "^0.26.1",
    "nexus-plugin-prisma": "^0.17.0",
    "nexus-prisma": "^0.16.2",
    "qrcode.react": "^1.0.0",
    "rc-picker": "^2.0.3",
    "react": "16.13.1",
    "react-csv": "^2.0.3",
    "react-dom": "16.13.1",
    "react-markdown": "4.3.1",
    "react-papaparse": "^3.7.1",
    "react-qr-scanner": "0.0.9",
    "react-sizeme": "^2.6.12",
    "react-to-print": "^2.9.0",
    "react-use": "^15.3.3",
    "sqlite3": "^5.0.0",
    "styled-components": "^5.1.1",
    "styled-media-query": "^2.1.2",
    "typescript-plugin-css-modules": "^2.4.0"
  },
  "devDependencies": {
    "@loadable/babel-plugin": "^5.13.0",
    "@types/graphql": "14.5.0",
    "@types/node": "14.0.27",
    "@types/react": "16.9.45",
    "@types/styled-components": "^5.1.2",
    "babel-plugin-styled-components": "^1.11.1",
    "copy-webpack-plugin": "^6.0.3",
    "husky": "^4.2.5",
    "null-loader": "^4.0.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "prisma-docs-generator": "0.0.5",
    "typescript": "3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
